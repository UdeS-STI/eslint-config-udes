{
  "name": "eslint-config-udes",
  "version": "0.4.1",
  "description": "ESLint shareable config for the UdeS JavaScript style guide",
  "keywords": [
    "check",
    "checker",
    "code",
    "code style",
    "enforce",
    "eslint",
    "eslintconfig",
    "hint",
    "jscs",
    "jshint",
    "jslint",
    "lint",
    "linter",
    "quality",
    "strict",
    "style",
    "udes",
    "validate",
    "verify"
  ],
  "homepage": "https://github.com/UdeS-STI/eslint-config-udes#readme",
  "bugs": {
    "url": "https://github.com/UdeS-STI/eslint-config-udes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/eslint-config-udes.git"
  },
  "license": "MIT",
  "author": "Universit√© de Sherbrooke",
  "main": "index.js",
  "files": [
    "index.js",
    "lit-html.js",
    "node-12.js",
    "polymer-2.js",
    "react.js"
  ],
  "scripts": {
    "format": "npm-run-all format:*",
    "format:eslint": "eslint --ignore-path .gitignore . --ext js,json,md --fix",
    "format:prettier": "prettier --ignore-path .gitignore --write '**/*.{js,json,md,yml}'",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore . --ext js,json,md",
    "lint:prettier": "prettier --ignore-path .gitignore --check '**/*.{js,json,md,yml}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index -g"
    }
  },
  "prettier": "prettier-config-udes",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "prettier-config-udes": "0.0.3"
  },
  "devDependencies": {
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "eslint-formatter-gitlab": "^2.0.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5"
  }
}
