{
  "name": "eslint-config-udes",
  "version": "0.2.0",
  "description": "ESLint shareable config for the UdeS JavaScript style guide",
  "main": "index.js",
  "scripts": {
    "format": "npm-run-all format:*",
    "format:js": "eslint . --ext js --fix",
    "format:json": "eslint . --ext json --fix",
    "format:markdown": "eslint . --ext md --fix",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --ext js",
    "lint:json": "eslint . --ext json",
    "lint:markdown": "eslint . --ext md"
  },
  "files": [
    "index.js",
    "node-8.js",
    "polymer-2.js",
    "polymer-2-application.js",
    "polymer-2-element.js",
    "react.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/eslint-config-udes.git"
  },
  "keywords": [
    "check",
    "checker",
    "code style",
    "code",
    "enforce",
    "eslint",
    "eslintconfig",
    "hint",
    "jscs",
    "jshint",
    "jslint",
    "lint",
    "linter",
    "quality",
    "strict",
    "style",
    "udes",
    "validate",
    "verify"
  ],
  "author": "Universit√© de Sherbrooke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/eslint-config-udes/issues"
  },
  "homepage": "https://github.com/UdeS-STI/eslint-config-udes#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.6.1"
  }
}
